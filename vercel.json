{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "env": {
        "NOME_DA_VARIAVEL": "@nome_da_variavel"
    },
    "engines": {
        "node": "14.x"
    }
}

